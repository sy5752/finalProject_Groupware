package kr.or.ddit.groupware.model;

import java.util.Date;

public class PmsVo {

	private int pms_no;
	private String app_char;
	private int emp_no;
	private int app_emp_no;
	private String pms_nm;
	private String cont;
	private Date s_dt;
	private Date e_dt;
	private Date est_time;
	private int prog;
	private int sta_cd;
	
	public PmsVo() {
		
	}
	
	public PmsVo(int pms_no, String app_char, int emp_no, int app_emp_no, String pms_nm, String cont, Date s_dt,
			Date e_dt, Date est_time, int prog, int sta_cd) {
		super();
		this.pms_no = pms_no;
		this.app_char = app_char;
		this.emp_no = emp_no;
		this.app_emp_no = app_emp_no;
		this.pms_nm = pms_nm;
		this.cont = cont;
		this.s_dt = s_dt;
		this.e_dt = e_dt;
		this.est_time = est_time;
		this.prog = prog;
		this.sta_cd = sta_cd;
	}
	public int getPms_no() {
		return pms_no;
	}
	public void setPms_no(int pms_no) {
		this.pms_no = pms_no;
	}
	public String getApp_char() {
		return app_char;
	}
	public void setApp_char(String app_char) {
		this.app_char = app_char;
	}
	public int getEmp_no() {
		return emp_no;
	}
	public void setEmp_no(int emp_no) {
		this.emp_no = emp_no;
	}
	public int getApp_emp_no() {
		return app_emp_no;
	}
	public void setApp_emp_no(int app_emp_no) {
		this.app_emp_no = app_emp_no;
	}
	public String getPms_nm() {
		return pms_nm;
	}
	public void setPms_nm(String pms_nm) {
		this.pms_nm = pms_nm;
	}
	public String getCont() {
		return cont;
	}
	public void setCont(String cont) {
		this.cont = cont;
	}
	public Date getS_dt() {
		return s_dt;
	}
	public void setS_dt(Date s_dt) {
		this.s_dt = s_dt;
	}
	public Date getE_dt() {
		return e_dt;
	}
	public void setE_dt(Date e_dt) {
		this.e_dt = e_dt;
	}
	public Date getEst_time() {
		return est_time;
	}
	public void setEst_time(Date est_time) {
		this.est_time = est_time;
	}
	public int getProg() {
		return prog;
	}
	public void setProg(int prog) {
		this.prog = prog;
	}
	public int getSta_cd() {
		return sta_cd;
	}
	public void setSta_cd(int sta_cd) {
		this.sta_cd = sta_cd;
	}
	@Override
	public String toString() {
		return "PmsVo [pms_no=" + pms_no + ", app_char=" + app_char + ", emp_no=" + emp_no + ", app_emp_no="
				+ app_emp_no + ", pms_nm=" + pms_nm + ", cont=" + cont + ", s_dt=" + s_dt + ", e_dt=" + e_dt
				+ ", est_time=" + est_time + ", prog=" + prog + ", sta_cd=" + sta_cd + "]";
	}
	
	
}
