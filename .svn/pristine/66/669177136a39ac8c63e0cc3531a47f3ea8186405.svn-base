	public void setJob_nm(List<String> job_nm) {
		String list = "";
		for(int i = 0; i<job_nm.size(); i++) {
			list += job_nm.get(i);
			if(i != job_nm.size()-1) {
				list += " / ";
			}
			
		}
		this.job_nm = list;
	}

